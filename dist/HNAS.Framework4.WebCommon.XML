            <param name="value">值</param>
            <returns></returns>
            <remarks>
            使用示列:
            XmlHelper.Insert(path, "/Node", "", "Value")
            XmlHelper.Insert(path, "/Node", "Attribute", "Value")
            </remarks>
        </member>
